<!--components/news-list/news-list.wxml-->
<view>
    <van-skeleton
        title
        row="3"
        loading="{{loading}}"
        wx:if="{{!list.length}}"
        wx:for="{{4}}"
        wx:key="item"
    >
    </van-skeleton>
    <list-item WX:else wx:for="{{listData}}" wx:key="item" news-obj="{{item}}"></list-item>
</view>
